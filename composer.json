{
    "name": "rz/media-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 Bundle SonataMediaBundle Extension",
    "keywords": ["symfony2", "Sonata Project", "RZProject"],
    "homepage": "https://github.com/rzproject/MediaBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "mell zamora",
            "email": "mell@rzproject.org",
            "homepage": "http://www.rzproject.org"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "rmzamora/media-bundle": "2.3.*@dev",
        "rz/admin-bundle": "2.3.1.*@dev",
        "rz/doctrine-orm-admin-bundle": "2.3.1.*@dev",
        "stof/doctrine-extensions-bundle": "1.1.*"
